#pragma OPENCL EXTENSION cl_amd_printf : enable 

// Summing float and int lists
__kernel void floatIntSum(__global int * source1, __global float * source2,__global float * dest,unsigned int size)
{
    unsigned int index = get_global_id(0);

    if (index < size) {
        dest[index] = source1[index] + source2[index];
    }
}
