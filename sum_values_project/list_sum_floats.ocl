#pragma OPENCL EXTENSION cl_amd_printf : enable 

// Summing two float lists
__kernel void floatSum(__global float * source1,__global float * source2, __global float * dest, unsigned int size)
{
    unsigned int index = get_global_id(0);

    if (index < size) {
        dest[index] = source1[index] + source2[index];
    }
}